{
  "expo": {
    "name": "Evomath",
    "slug": "Evomath",
    "privacy": "public",
    "description": "Evomath - Brought to you by Chronos",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./expo-assets/icon.png",
    "splash": {
      "image": "./expo-assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./expo-assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./expo-assets/favicon.png"
    },
    "githubUrl": "https://github.com/markosneiders/Evomath"
  }
}
